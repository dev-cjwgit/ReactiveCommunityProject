<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cjw.reactivecommunityproject.server.cache.info.data.mapper.CacheInfoDataMapper">
    <select id="selectCommonRegionList" resultType="CacheInfoDataCommonRegionVO">
        /* NOT_SQL_LOG */
        SELECT `region`
             , `enabled`
             , `updated_utc_at`
        FROM rc_common_region
    </select>

    <select id="selectCommonEnvCodeList" parameterType="String" resultType="CacheInfoDataCommonEnvCodeVO">
        /* NOT_SQL_LOG */
        SELECT `region`
             , `id`
             , `type`
             , `value`
             , `order`
             , `category`
             , `enabled`
             , `updated_utc_at`
        FROM rc_common_env_code
        WHERE `region` = '${region}'
    </select>

    <select id="selectCommonLanguageCodeList" resultType="CacheInfoDataCommonLanguageCodeVO">
        /* NOT_SQL_LOG */
        SELECT `path`
             , `code`
             , `value`
             , `updated_utc_at`
        FROM rc_common_language_code
    </select>

    <select id="selectCommonLanguageGbCodeList" resultType="CacheInfoDataCommonLanguageGbCodeVO">
        /* NOT_SQL_LOG */
        SELECT `path`
             , `code`
             , `language`
             , `value`
             , `updated_utc_at`
        FROM rc_common_language_gb_code
    </select>

    <select id="selectManageResourceList" resultType="CacheInfoDataManageResourceVO">
        /* NOT_SQL_LOG */
        SELECT `uid`
             , `method`
             , `url_pattern`
             , `enabled`
             , `updated_utc_at`
        FROM rc_manage_resource
    </select>

    <select id="selectManageFunctionList" resultType="CacheInfoDataManageFunctionVO">
        /* NOT_SQL_LOG */
        SELECT `uid`
             , `name`
             , `type`
             , `enabled`
             , `updated_utc_at`
        FROM rc_manage_function
    </select>

    <select id="selectManageRoleFunctionList" resultType="CacheInfoDataManageRoleFunctionVO">
        /* NOT_SQL_LOG */
        SELECT `role_uid`
             , `function_uid`
             , `enabled`
             , `updated_utc_at`
        FROM rc_manage_role_function
    </select>

    <select id="selectManageRoleResourceList" resultType="CacheInfoDataManageRoleResourceVO">
        /* NOT_SQL_LOG */
        SELECT `role_uid`
             , `resource_uid`
             , `enabled`
             , `updated_utc_at`
        FROM rc_manage_role_resource
    </select>
</mapper>